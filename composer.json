{
  "name": "wgg/pimcore-mailchimp",
  "type": "pimcore-bundle",
  "license": "Apache-2.0",
  "description": "Pimcore Mailchimp Bundle",
  "keywords": [
    "pimcore",
    "mailchimp"
  ],
  "homepage": "https://github.com/WebGardenGroup/mailchimp-bundle",
  "authors": [
    {
      "name": "Webgarden Developer",
      "email": "developer+github@webgarden.eu",
      "homepage": "https://webgarden.eu/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wgg\\MailchimpBundle\\": "src/MailchimpBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Wgg\\MailchimpBundle\\Tests\\": "tests/"
    },
    "classmap": [
      "tests/project/app/AppTestKernel.php"
    ]
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Wgg\\MailchimpBundle\\WggMailchimpBundle"
      ]
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "pimcore/pimcore": "^6.0",
    "mailchimp/marketing": "^3.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-symfony": "^0.12",
    "friendsofphp/php-cs-fixer": "^2.18",
    "symfony/phpunit-bridge": "^4.4|^5.2",
    "phpstan/phpstan-phpunit": "^0.12.18"
  },
  "scripts": {
    "test": [
      "@composer install --prefer-dist --ignore-platform-reqs",
      "simple-phpunit"
    ],
    "phpstan": [
      "@composer install --prefer-dist --ignore-platform-reqs",
      "simple-phpunit install",
      "@phpstan-general",
      "@phpstan-tests"
    ],
    "phpstan-general": [
      "phpstan analyse -c phpstan.neon"
    ],
    "phpstan-tests": [
      "phpstan analyse -c phpstan.tests.neon"
    ]
  },
  "archive": {
    "exclude": [
      "docs"
    ]
  }
}
