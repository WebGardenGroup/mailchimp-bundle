{
  "name": "wgg/mailchimp-bundle",
  "type": "pimcore-bundle",
  "license": "Apache-2.0",
  "description": "Pimcore Mailchimp Bundle",
  "keywords": [
    "pimcore",
    "mailchimp"
  ],
  "homepage": "https://github.com/WebGardenGroup/mailchimp-bundle",
  "authors": [
    {
      "name": "WebgardenGroup",
      "email": "developer@webgarden.eu",
      "homepage": "https://webgarden.eu/",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Wg\\MailchimpBundle\\": "src/MailchimpBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "": "src/"
    }
  },
  "extra": {
    "pimcore": {
      "bundles": [
        "Wg\\MailchimpBundle\\WgMailchimpBundle"
      ]
    }
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "pimcore/pimcore": "^6.0",
    "mailchimp/marketing": "^3.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-symfony": "^0.12",
    "friendsofphp/php-cs-fixer": "^2.18"
  },
  "scripts": {
    "phpstan": [
      "@composer install",
      "@phpstan-general",
      "@phpstan-tests"
    ],
    "phpstan-general": [
      "bin/console cache:warmup --env=dev",
      "phpstan analyse -c phpstan.neon"
    ]
  }
}
